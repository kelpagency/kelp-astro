	<style lang="scss">
		.services {
			padding: 6rem 0;
		}
		.service {
			margin: 10rem 0 8rem;
			display: flex;
			gap: 1rem 3rem;
			flex-wrap: wrap;
			@media (min-width: 768px) {
				padding-left: 70px;
				padding-right: 70px;
			}
			&-left {
				flex: 1 1 200px;
				position: relative;
				text-align: right;
				margin-top: -5rem;
				h3 {
					text-align: left;
					margin: 0;
				}
				&:before {
					content: "";
					width: 108px;
					height: 108px;
					display: block;
					position: absolute;
					right: 0;
					z-index: -1;
					top: 2rem;
					background: var(--light-teal);
					.service--marketing & {
						background: var(--light-green);
						right: 1rem;
					}
					.service--ongoing & {
						background: var(--light-red);
					}
				}
				svg {
					margin-right: 1rem;
				}
			}
			&-right {
				flex: 1 1 400px;
				ul {
					column-count: 2;
					list-style: none;
					padding: 0;
					margin-top: 2rem;
					li {
						line-height: 1.2;
						font-weight: bold;
						text-decoration: underline;
						+ li {
							margin-top: 1rem;
						}
					}
				}
			}
		}
	</style>
